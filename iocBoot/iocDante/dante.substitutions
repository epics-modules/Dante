{   P,      M,       DTYP,         INP,         PREC,    NCHAN}
{Dante:,   mca1,  "asynMCA",  "@asyn(DANTE1,0)",   2,     2048}
}

file "$(ASYN)/db/asynRecord.db"
{
pattern
{P,             R,        PORT,   ADDR,  IMAX,  OMAX}
{Dante:, dante1:AsynIO, DANTE1,      0,    80,    80}
}
